    "    for  i in range(1,10):\n",
    "        for j in range(n, n+4):\n",
    "            print('%dX%d=%2d'%(j,i,i*j), end=' ')\n",
    "        print()\n",
    "    print()\n",
    "print('구구클래스 재현');guguclass(2);guguclass(6);"
